server {
#	listen 443  default_server;
        listen 80; 
        server_name salent.vip;
      #  allow all;
      #  ssl on;
      #  ssl_certificate /root/projects/ssl/www.salent.vip.pem;
      #  ssl_certificate_key /root/projects/ssl/www.salent.vip.key;
        location /static{
                alias ~/projects/ToDo/ToDo/static;
        }
        location /{
                include uwsgi_params;
                uwsgi_pass 127.0.0.1:8032;
        }
}

